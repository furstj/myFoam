FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    INLET
    {
      type            flowRateInletVelocity;
      massFlowRate 0.3473;
      value $internalField;
    }
    OUTLET
    {
        type            zeroGradient;
    }
    STATOR
    {
        type            empty;
    }
    BLADE_ROT
    {
        type            fixedValue;
        value           uniform (0 0 0);    
    }
    BLADE_STAT
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    ROTOR 
    {
        type            symmetryPlane;
    }
    AMI_INT
    {
	type		cyclicAMI;
    }
    AMI_EXT
    {
	type		cyclicAMI;
    }
    HUB_SHROUD
    {
	type		empty;
    }
}
// ************************************************************************* //

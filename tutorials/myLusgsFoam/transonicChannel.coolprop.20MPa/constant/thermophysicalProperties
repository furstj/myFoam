FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            hePsiThermo;
    mixture         pureMixture;
    transport       CoolProp;
    thermo          CoolProp; 
    equationOfState CoolPropGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    CoolProp
    {
        backend  BICUBIC&HEOS;
        fluid    N2;
    }
}


